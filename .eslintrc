{
  "extends": ["standard", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "prettier/prettier": "error",
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-console": 0,
    "eqeqeq": 0,
    "camelcase": [0]
  }
}
